{
    "name": "board3r/mistral-laravel-plugin",
    "description": "Laravel plugin for Mistral SDK",
    "type": "laravel-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Board3r\\MistralLaravelPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Maxime",
            "email": "maxime.danjou31@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.19",
        "board3r/mistral-sdk": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.7"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
